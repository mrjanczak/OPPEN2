
$('#file_list_Year').change(function(e) {
	var year_id  = $(this).find('option:selected').val();
	 $.ajax({
		type: "POST",
		url: $(this).parents('.filter').data('url').replace(/__year_id__/g, year_id),
		success: function(data){
			console.log( JSON.stringify(data) );
			$('#file_list_FileCat option').remove();
			$('#file_list_FileCat').append( data[0].join());
		}
	});
});





